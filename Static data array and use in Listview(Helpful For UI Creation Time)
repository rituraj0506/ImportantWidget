      child: Row(
        crossAxisAlignment: CrossAxisAlignment.start,
        children: [

          Column(
            crossAxisAlignment: CrossAxisAlignment.start,
            children: [
              Text(item['startDate'], style: TextStyle(fontWeight: FontWeight.bold)),
              Text(item['subjectâ€™],))

           ])])




