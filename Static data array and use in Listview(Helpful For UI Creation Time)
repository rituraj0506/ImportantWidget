  },
  {
    'startDate': '25 May',
    'endDate': '29 May 24',
    'subject': 'Hindi',
    'type': 'Class Test',
    'teacher': 'Jenifer Clark',
    'teacherImage': 'assets/teacher2.png',
  },
];

